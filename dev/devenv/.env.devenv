{
    "envs": {
        "default": {
            "alias-for": "test"
        },
        "test": {
            "vars": {
                "UMBRA_SANITISE": "ON",
                "UMBRA_LINT": "ON",
                "PATH_PREPEND": "{{git_root}}/build/bin"
            }
        },
        "prod": {
            "vars": {
                "UMBRA_SANITISE": "OFF",
                "UMBRA_LINT": "ON",
                "PATH_PREPEND": "{{git_root}}/build/bin"
            }
        }
    }
}
// vim:ft=jsonc
